  "author": {
    "name": "Thorn Wu",
    "url": "https://github.com/ThornWu"
  },
  "contributors": [
    {
      "name": "Thorn Wu",
      "url": "https://github.com/ThornWu"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/ThornWu/jira-xml-parser",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThornWu/jira-xml-parser.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
