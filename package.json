    "@types/sax": "^1.2.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.0.2",
    "typescript": "^5.8.3",
    "@types/mocha": "^10.0.6",
    "@types/chai": "^4.3.11",
    "chai": "^4.4.1",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0"
